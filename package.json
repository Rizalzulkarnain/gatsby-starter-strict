{
  "private": true,
  "name": "gatsby-starter-strict",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,js,json,yml,md}\"",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "lint:style": "stylelint --ignore-path .gitignore \"src/**/*.{ts,tsx,js}\"",
    "serve": "gatsby serve",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@rhysforyou/gatsby-plugin-react-helmet-async": "^0.1.1",
    "emotion-theming": "^10.0.19",
    "gatsby": "^2.17.6",
    "gatsby-plugin-emotion": "^4.1.13",
    "gatsby-plugin-manifest": "^2.2.25",
    "gatsby-plugin-typescript": "^2.1.15",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet-async": "^1.0.4"
  },
  "devDependencies": {
    "@kripod/eslint-config-react": "^1.0.5",
    "@kripod/eslint-config-typescript": "^2.0.2",
    "@testing-library/react": "^9.3.0",
    "@testing-library/react-hooks": "^3.1.1",
    "@types/jest": "^24.0.20",
    "@types/react": "^16.9.11",
    "@types/react-helmet": "^5.0.13",
    "eslint": "^6.6.0",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-testing-library": "^1.3.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.17",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.11.0",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^3.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  }
}
