{
  "private": true,
  "name": "gatsby-starter-strict",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,js,json,yml,md}\"",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "lint:style": "stylelint --ignore-path .gitignore \"src/**/*.{ts,tsx,js}\"",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/core": "^10.0.20",
    "@emotion/styled": "^10.0.17",
    "@rhysforyou/gatsby-plugin-react-helmet-async": "^0.1.1",
    "emotion-theming": "^10.0.19",
    "gatsby": "^2.15.28",
    "gatsby-plugin-emotion": "^4.1.9",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-typescript": "^2.1.11",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet-async": "^1.0.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.4",
    "@types/react-helmet": "^5.0.11",
    "@types/react-test-renderer": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^2.1.2",
    "eslint-plugin-testing-library": "^1.0.0",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.17",
    "lint-staged": "^9.4.1",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2",
    "stylelint": "^11.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^3.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
